{
  "family": "rachael-task",
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::882424314092:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "rachaels-consciousness",
      "image": "882424314092.dkr.ecr.us-west-1.amazonaws.com/deckards/replicant-repository:rachaels-consciousness",
      "memory": 128,
      "cpu": 128,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/rachael",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "consciousness",
          "awslogs-create-group": "true"
        }
      }
    }
  ],
  "requiresCompatibilities": ["EC2"]
}
